 <%- include("partials/header") %> 
<div class="container text-center">
	<h1 class="pagename mt-4">Welcome, Please Login!</h1>
	<div class="row justify-content-center">
		<div class="col-md-6">
			<form action="/login" method="POST">
				<input class="username form-control rounded my-2 shadow-sm" type="text" name="username" placeholder="username" required>
				<input class="password form-control rounded my-2 shadow-sm" type="password" name="password" placeholder="password" required>
				<button class="btn btn-success my-2 login">Login</button>
			</form>
			<a href="/register" class="btn btn-info signup">Sign Up</a>
		</div>
	</div>

</div>




<%- include("partials/footer") %>